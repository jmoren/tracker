= form_for @project do |f|
  = f.error_messages
  %p
    = f.label :name
    %br
    = f.text_field :name
  %p
    = f.label :description
    %br
    = f.text_area :description
  %p
    = f.label :start_date
    %br
    = f.date_select :start_date
  %p
    = f.label :end_date
    %br
    = f.date_select :end_date
  %p
    = f.label :status
    %br
    = f.text_field :status
  %p
    = f.label :user_id
    %br
    = f.text_field :user_id
  %p
    = f.submit
