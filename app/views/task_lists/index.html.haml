- title "Task Lists"

%table
  %tr
    %th Project
    %th Title
  - for task_list in @task_lists
    %tr
      %td= task_list.project_id
      %td= task_list.title
      %td= link_to 'Show', task_list
      %td= link_to 'Edit', edit_task_list_path(task_list)
      %td= link_to 'Destroy', task_list, :confirm => 'Are you sure?', :method => :delete

%p= link_to "New Task List", new_task_list_path
